<?xml version="1.0" encoding="UTF-8" ?>
<VfeConfigMsg product="ginger-cheevers" enableFrontDoor="true" lowPowerTimeout="1100">
  <aecConf num_mics="8" num_refs="3" num_control="1" sys_delay="0" mic_delay="512" mic_scale_factor="32768" ref_scale_factor="32768" >
    <threadCfg affinity="2" priority="48" />
  </aecConf>
  <alsaConf deviceName="hw:0,1" sampleRate="16000" periodSize="192" bufferSize="1536" numChannels="8" />
  <timeAlignConf num_secondary_channels="4" num_sync_frames="12" timing_tolerance="2000" primary_buffer_size="1024000" secondary_buffer_size="1024000" >
    <threadCfg affinity="2" priority="50" />
  </timeAlignConf>
  <meterConf num_channels_in="8" num_channels_out="8" mic_sensitivity="-27" lib="./ginger-cheevers/libVfeMeteringElement.so" >
    <threadCfg affinity="4" priority="48" />
  </meterConf>
  <vpaConfig wuwId="1" vpaName="Alexa" >
    <elementCfg name="Beamformer" 
                pathToLib="/opt/Bose/bin/VfeElementLibs/Beamformer/ginger-cheevers/libVfeBeamformer.so"
                createFuncSymbol="create_beam_former"
                pathToConfig="/opt/Bose/etc/VfeElementCfgs/Beamformer/ginger-cheevers/BeamformerConfig.xml" />
    <elementCfg name="AmazonWuw"
                pathToLib="/opt/Bose/bin/VfeElementLibs/WuwElement/libAmazonWuw.so"
                createFuncSymbol="create_amazon_wuw"
                pathToConfig="/opt/Bose/etc/VfeElementCfgs/WuwElement/ginger-cheevers/AmazonWuwConfig.xml" />
  </vpaConfig>
  <vpaConfig wuwId="2" vpaName="Google" >
    <elementCfg name="Beamformer" 
                pathToLib="/opt/Bose/bin/VfeElementLibs/Beamformer/ginger-cheevers/libVfeBeamformer.so"
                createFuncSymbol="create_beam_former"
                pathToConfig="/opt/Bose/etc/VfeElementCfgs/Beamformer/ginger-cheevers/BeamformerConfig.xml" />
    <elementCfg name="GoogleHotWord"
                pathToLib="/opt/Bose/bin/VfeElementLibs/WuwElement/libGoogleHotWord.so"
                createFuncSymbol="create_google_wuw"
                pathToConfig="/opt/Bose/etc/VfeElementCfgs/WuwElement/ginger-cheevers/GoogleHotWordConfig.xml" />
  </vpaConfig>
  <vpaConfig wuwId="3" vpaName="Siri" >
    <elementCfg name="Beamformer" 
                pathToLib="/opt/Bose/bin/VfeElementLibs/Beamformer/ginger-cheevers/libVfeBeamformer.so"
                createFuncSymbol="create_beam_former"
                pathToConfig="/opt/Bose/etc/VfeElementCfgs/Beamformer/ginger-cheevers/BeamformerConfig.xml" />
    <elementCfg name="SiriPtt"
                pathToLib="/opt/Bose/bin/VfeElementLibs/WuwElement/libSiriPtt.so"
                createFuncSymbol="create_siri_wuw"
                pathToConfig="/opt/Bose/etc/VfeElementCfgs/WuwElement/ginger-cheevers/SiriPttConfig.xml" />
  </vpaConfig>
  <vpaConfig wuwId="4" vpaName="AlexaComms" >
    <elementCfg name="BeamformerComms" 
                pathToLib="/opt/Bose/bin/VfeElementLibs/Beamformer/ginger-cheevers/libVfeBeamformerComms.so"
                createFuncSymbol="create_beam_former"
                pathToConfig="/opt/Bose/etc/VfeElementCfgs/Beamformer/ginger-cheevers/BeamformerConfig-comms.xml" />
    <elementCfg name="AlexaComms"
                pathToLib="/opt/Bose/bin/VfeElementLibs/WuwElement/libAlexaComms.so"
                createFuncSymbol="create_alexa_comms"
                pathToConfig="/opt/Bose/etc/VfeElementCfgs/WuwElement/ginger-cheevers/AlexaComms.xml" />
  </vpaConfig>
</VfeConfigMsg>

