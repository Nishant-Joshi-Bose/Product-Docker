{
  "acoustic-model": {
    "non-speech-pdf": -2,
    "speech-pdf": -1
  },
  "keyword-orchestra": {
    "buffer-size": 200,
    "keyword-spotters": [
      {
        "classifier": {
          "type": "vector-space",
          "vector-space": {
            "vector-space-classifier": {
              "map": [
                {
                  "classifier": {
                    "normalized-vector-space-classifier": {
                      "classifier": {
                        "svm": {
                          "filename": "ALEXA.psvm",
                          "probabilistic": false
                        },
                        "type": "svm"
                      },
                      "normalizer": {
                        "scale": {
                          "filename": "ALEXA.scales"
                        },
                        "type": "scale"
                      }
                    },
                    "type": "normalized-vector-space-classifier"
                  },
                  "name": "ALEXA"
                }
              ],
              "type": "map"
            },
            "vector-space-mapper": {
              "type": "version1",
              "version1": {
                "add-context-features": true,
                "log-energy-feature-begin-index": 400,
                "log-energy-feature-end-index": 420,
                "sns-smoothing-context": 5,
                "sns-speech-prior": 0.5
              }
            }
          }
        },
        "hmm": {
          "bg": {
            "filename": "ALEXA.bg.hclg.pfst",
            "type": "pryon-fst"
          },
          "fg": {
            "filename": "ALEXA.fg.hclg.pfst",
            "type": "pryon-fst"
          },
          "search-space": {
            "beam": 25.0,
            "block-size": 1024,
            "max-depth": 200
          },
          "window-size": 6
        },
        "name": "ALEXA",
        "spotter": {
          "classification-limits": {
            "max-per-window": 3,
            "stickiness": 0,
            "window-size": 100
          },
          "classification-thresholds": {
            "accept-threshold": -0.3
          },
          "cleanup-period": 6000,
          "escalation-period": 200,
          "hmm-thresholds": {
            "accept-threshold": 0.0
          },
          "lock-period": 40,
          "probabilistic-hmm-near-miss": {
            "decay": 15,
            "enabled": false
          }
        }
      },
      {
        "classifier": {
          "type": "vector-space",
          "vector-space": {
            "vector-space-classifier": {
              "trivial": {
                "score": 0.0
              },
              "type": "trivial"
            },
            "vector-space-mapper": {
              "type": "version1",
              "version1": {}
            }
          }
        },
        "hmm": {
          "bg": {
            "filename": "STOP.bg.hclg.pfst",
            "type": "pryon-fst"
          },
          "fg": {
            "filename": "STOP.fg.hclg.pfst",
            "type": "pryon-fst"
          },
          "search-space": {
            "beam": 25.0,
            "block-size": 1024,
            "max-depth": 200
          }
        },
        "name": "STOP",
        "spotter": {
          "classification-limits": {
            "max-per-window": 3,
            "stickiness": 0,
            "window-size": 100
          },
          "classification-thresholds": {
            "accept-threshold": 0.0
          },
          "cleanup-period": 6000,
          "escalation-period": 200,
          "hmm-thresholds": {
            "accept-threshold": 0.0
          },
          "lock-period": 40
        }
      }
    ]
  },
  "object_type_name": "keyword-spotter-orchestra-config",
  "object_type_version": 2,
  "phone-symbol-table": {
    "filename": "phones.txt"
  },
  "word-symbol-table": {
    "filename": "words.shrunk.txt"
  }
}
