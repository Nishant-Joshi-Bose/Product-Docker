<ShepherdConfig>
  <daemon name="Eddie"/>

  <daemon name="Monaco"
          exe="/opt/Bose/webkit/run_wpe.sh">
    <arg>MONACO</arg>
    <arg>1.0</arg>
    <arg>PORTRAIT</arg>
  </daemon>

  <daemon name="avahi-daemon"
          exe="start-avahi"/>

  <daemon name="IoT"/>

  <daemon name="UpnpSource">
    <arg>-c/opt/Bose/etc/UpnpSource.xml</arg>
  </daemon>

  <daemon name="CAPS"
          recovery="reboot">
    <arg>-c/opt/Bose/etc/CAPS.xml</arg>
  </daemon>

  <daemon name="STSCertified"/>

  <daemon name="PtsServer">
    <arg>/opt/Bose/etc/pts-handler</arg>
  </daemon>

</ShepherdConfig>
